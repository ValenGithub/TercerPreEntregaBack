
   <div class="container">
    <h1>Admin Control de Usuarios</h1>
    <table>
        <thead>
            <tr>
                <th>Email</th>
                <th>Rol</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr>
                <td>{{this.email}}</td>
                <td>{{this.rol}}</td>
                
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
